<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="WorkTimeType">
    <!-- 근무현황 기록용 유형 목록 가져오기 -->
    <select id="getAllWorkTimeTypes" resultType="com.kedu.ggirick_client_backend.dto.workmanagement.WorkTimeTypeDTO">
        SELECT
        type,
        name
        FROM work_time_type
    </select>
</mapper>